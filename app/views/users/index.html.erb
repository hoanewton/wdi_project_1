<h3>Users</h3>

<% @users.each do |user| %>
<hr>
<%= user.id %><br>
<b><%= user.username %></b><br>
<%= "#{user.first_name} #{user.last_name}" %><br>
 <!-- if current_user && current_user.id == user.id %> -->
	<%= link_to 'Edit', edit_user_path(user) %>
	<%= link_to 'Delete', user_path(user), method: :delete %>
 <!-- end %><hr> -->

<% end %>